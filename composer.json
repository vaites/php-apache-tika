{
    "name": "vaites/php-apache-tika",
    "description": "Apache Tika bindings for PHP: extracts text from documents and images (with OCR), metadata and more...",
    "keywords": ["apache", "tika", "pdf", "doc", "docx", "ppt", "pptx", "odt", "office", "documents", "ocr"],
    "license": "MIT",
    "authors": [
        {
            "name": "David MartÃ­nez",
            "email": "contacto@davidmartinez.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vaites\\ApacheTika\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Vaites\\ApacheTika\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "nunomaduro/phpinsights": "^2.2.0",
        "phpstan/phpstan": "^1.6.2",
        "symfony/var-dumper": "^5.4|^6.0",
        "filp/whoops": "^2.7",
        "psy/psysh": "^0.11.17",
        "nunomaduro/collision": "^5.11"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse -c phpstan.neon",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@php -S localhost:8000 -t ."
        ],
        "test": "vendor/bin/phpunit --coverage-html reports"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
